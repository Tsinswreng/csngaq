<UserControl xmlns="https://github.com/avaloniaui"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
		xmlns:vm="clr-namespace:ngaq.UI.ViewModels"
		x:Class="ngaq.UI.Views.FullWordKvView.FullWordKvView"
		x:DataType="vm:FullWordKv.FullWordKvVm"
		xmlns:KvView="clr-namespace:ngaq.UI.Views.KV"
>
	<Design.DataContext>

	</Design.DataContext>

	<StackPanel Orientation="Horizontal">
		<StackPanel Orientation="Vertical">
			<TextBlock Text="textWord" />
			<Button Content="print" Click="printChangedModel_Click"/>
			<KvView:KvView
				WordKv="{Binding textWord, Mode=TwoWay}"
				Name="textWord"
			/>
		</StackPanel>

		<StackPanel Orientation="Vertical">
			<Button Content="print" Click="printChangedModel_Click2"/>
			<TextBlock Text="propertys[0]" />
			<KvView:KvView DataContext="{Binding textWordVm}"/>
		</StackPanel>

		<StackPanel Orientation="Vertical">
			<TextBlock Text="learns[0]" />
			<KvView:KvView />
		</StackPanel>

	</StackPanel>
</UserControl>